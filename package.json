{
  "name": "payment-system",
  "version": "1.0.0",
  "description": "Core GU API",
  "scripts": {
    "local": "SLS_DEBUG=* && serverless offline --verbose --port=8888 --stage local --region us-west-2",
    "deploy:test": "SLS_DEBUG=* && serverless deploy --verbose --stage test && serverless prune -n 3 --stage test --region us-west-2 --aws-profile=prod",
    "undeploy:test": "SLS_DEBUG=* && serverless remove --verbose --stage test --region us-east-2 --aws-profile=prod",
    "undeploy": "sls remove",
    "package": "sls package",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "test:all": "jest",
    "test:unit": "jest --group=unit --passWithNoTests",
    "test:int": "jest --group=integration",
    "verify": "run-s lint test:unit",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\""
  },
  "keywords": [],
  "author": "Viktor Perdomo",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-transform-async-to-generator": "^7.13.0",
    "@babel/plugin-transform-instanceof": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.10.4",
    "@serverless/eslint-config": "^3.0.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "jest": "^26.1.0",
    "jest-runner-groups": "^2.0.1",
    "jest-webpack": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "npm-scripts-info": "^0.3.9",
    "prettier": "^2.2.1",
    "serverless": "2.47.0",
    "serverless-domain-manager": "5.1.0",
    "serverless-offline": "6.9.0",
    "serverless-plugin-split-stacks": "1.9.3",
    "serverless-plugin-warmup": "5.3.1",
    "serverless-prune-plugin": "1.4.3",
    "serverless-webpack": "5.5.1",
    "webpack": "5.39.1",
    "webpack-cli": "4.7.2",
    "webpack-node-externals": "2.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.7",
    "aws-sdk": "^2.893.0",
    "lambda-log": "1.4.0",
    "luxon": "^2.0.2",
    "md5": "2.2.1",
    "moment": "^2.26.0",
    "request": "2.85.0",
    "request-promise": "4.2.2",
    "shopify-api-node": "2.19.2",
    "uuid": "^8.3.2",
  }
